{
  "name": "vite-preload",
  "version": "1.0.0",
  "description": "Speed up your Vite application by preloading server rendered lazy modules and stylesheets as early as possible",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./__internal": {
      "import": "./dist/__internal.js",
      "require": "./dist/__internal.cjs"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "require": "./dist/plugin.cjs"
    }
  },
  "type": "module",
  "files": [
    "./dist"
  ],
  "scripts": {
    "test": "tsc",
    "build": "rollup -c rollup.config.js"
  },
  "author": "wille",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wille/vite-preload.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/babel__parser": "^7.0.0",
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^22.3.0",
    "@types/react": "^18.3.3",
    "prettier": "^3.3.3",
    "rollup": "^4.20.0",
    "tslib": "^2.6.3",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": ">= 18",
    "vite": ">= 5"
  },
  "dependencies": {
    "@babel/generator": "^7.25.0",
    "@babel/parser": "^7.25.3",
    "@babel/traverse": "^7.25.3",
    "@babel/types": "^7.25.2",
    "mime": "^4.0.4",
    "react-lazy-with-preload": "^2.2.1"
  },
  "keywords": [
    "react",
    "css",
    "ssr",
    "esmodules",
    "vite",
    "vite-plugin"
  ]
}
